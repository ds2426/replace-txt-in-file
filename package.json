{
	"name": "growers",
	"version": "1.0.0",
	"description": "",
	"main": "./src/index.tsx",
	"scripts": {
		"start": "yarn tsc:enums && webpack serve --mode development --hot",
		"build": "yarn tsc:enums && webpack --mode production",
		"tsc": "tsc --noEmit --skipLibCheck",
		"tsc:enums": "rm -rf ./dist && tsc --project tsconfig-enums.json --skipLibCheck",
		"lint": "npm run lint:ts && npm run lint:scss",
		"lint:ts": "eslint 'src/**/*.{ts,tsx}'",
		"lint:scss": "stylelint './src/**/*.scss' --config .stylelintrc",
		"test": "jest",
		"test:coverage": "jest --coverage",
		"run-dist": "yarn build && npx ecstatic ./dist --root=./dist --host=localhost --port=8080 --baseDir=/",
		"postbuild": "workbox generateSW"
	},
	"license": "MIT",
	"dependencies": {
		"@loadable/component": "5.15.0",
		"@types/react-hot-loader": "^4.1.1",
		"axios": "0.24.0",
		"connected-react-router": "6.9.1",
		"date-fns": "2.25.0",
		"fs": "^0.0.1-security",
		"history": "4.10.1",
		"normalize.css": "8.0.1",
		"react": "17.0.2",
		"react-dom": "17.0.2",
		"react-hook-form": "7.20.0",
		"react-hot-loader": "^4.13.0",
		"react-redux": "7.2.6",
		"react-router": "5.2.1",
		"react-router-dom": "5.3.0",
		"redux": "4.1.2",
		"redux-devtools-extension": "2.13.9",
		"replace-in-file": "^6.3.2",
		"scss-goodies": "1.3.0"
	},
	"devDependencies": {
		"@dreysolano/prerender-spa-plugin": "1.0.3",
		"@loadable/webpack-plugin": "5.15.1",
		"@pmmmwh/react-refresh-webpack-plugin": "0.5.2",
		"@types/enzyme": "3.10.10",
		"@types/jest": "27.0.3",
		"@types/loadable__component": "5.13.4",
		"@types/node": "16.11.8",
		"@types/react": "17.0.35",
		"@types/react-dom": "17.0.11",
		"@types/react-loadable": "5.5.6",
		"@types/react-redux": "7.1.20",
		"@types/react-router": "5.1.17",
		"@types/react-router-dom": "5.3.2",
		"@typescript-eslint/eslint-plugin": "5.4.0",
		"@typescript-eslint/parser": "5.4.0",
		"@wojtekmaj/enzyme-adapter-react-17": "0.6.5",
		"autoprefixer": "10.4.0",
		"copy-webpack-plugin": "10.0.0",
		"css-hot-loader": "1.4.4",
		"css-loader": "6.5.1",
		"cssnano": "5.0.11",
		"dotenv": "10.0.0",
		"enzyme": "3.11.0",
		"enzyme-to-json": "3.6.2",
		"eslint": "8.2.0",
		"eslint-config-prettier": "8.3.0",
		"eslint-plugin-jest": "25.2.4",
		"eslint-plugin-react": "7.27.0",
		"html-loader": "3.0.1",
		"html-webpack-plugin": "5.5.0",
		"jest": "27.3.1",
		"mini-css-extract-plugin": "2.4.5",
		"postcss": "8.3.11",
		"postcss-easy-import": "3.0.0",
		"postcss-flexbugs-fixes": "5.0.2",
		"postcss-loader": "6.2.0",
		"postcss-url": "10.1.3",
		"postcss-utilities": "0.8.4",
		"prerender-spa-plugin": "3.4.0",
		"react-refresh": "0.11.0",
		"react-refresh-typescript": "2.0.2",
		"sass": "1.43.4",
		"sass-loader": "12.3.0",
		"sass-resources-loader": "2.2.4",
		"style-loader": "3.3.1",
		"stylelint": "14.1.0",
		"stylelint-config-recommended": "6.0.0",
		"stylelint-config-standard-scss": "2.0.1",
		"stylelint-no-unsupported-browser-features": "5.0.2",
		"stylelint-scss": "4.0.0",
		"svgo-viewbox": "2.0.4",
		"terser-webpack-plugin": "5.2.5",
		"ts-jest": "27.0.7",
		"ts-loader": "9.2.6",
		"tslib": "2.3.1",
		"typescript": "4.5.2",
		"webpack": "5.64.1",
		"webpack-cli": "4.9.1",
		"webpack-dev-server": "4.5.0",
		"webpack-hot-middleware": "2.25.1",
		"webpack-pwa-manifest": "4.3.0",
		"workbox-cli": "6.4.1",
		"yargs": "17.2.1"
	},
	"browserslist": [
		"> 1%",
		"last 2 versions"
	],
	"jest": {
		"moduleDirectories": [
			"node_modules",
			"src"
		],
		"moduleFileExtensions": [
			"ts",
			"tsx",
			"js",
			"jsx",
			"json",
			"node"
		],
		"moduleNameMapper": {
			"^@src/(.*)$": "<rootDir>/src/$1",
			"^@root/(.*)$": "<rootDir>/$1",
			"^@assets/(.*)$": "<rootDir>/src/assets/$1",
			"^@utilities": "<rootDir>/src/utilities",
			"^@utilities/(.*)$": "<rootDir>/src/utilities/$1",
			"^@components": "<rootDir>/src/components",
			"^@pages/(.*)$": "<rootDir>/src/pages/$1",
			"\\.(css|less|sass|scss)$": "<rootDir>/test-config/StyleMock.js",
			"\\.(jpg|jpeg|png|gif|ico|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/test-config/FileMock.js"
		},
		"setupFiles": [
			"<rootDir>/test-config/index.js"
		],
		"snapshotSerializers": [
			"enzyme-to-json/serializer"
		],
		"transform": {
			"^.+\\.tsx?$": "ts-jest",
			"\\.(css|less|sass|scss)$": "<rootDir>/test-config/StyleMock.js",
			"\\.(jpg|jpeg|png|gif|ico|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/test-config/FileMock.js"
		},
		"verbose": false,
		"testRegex": ".*\\.(test|spec)\\.(j|tsx?)$",
		"testPathIgnorePatterns": [
			"<rootDir>/node_modules/",
			"<rootDir>/dist/",
			"<rootDir>/test-config/"
		]
	}
}
